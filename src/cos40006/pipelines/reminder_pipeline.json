{
    "version": 0,
    "name": "reminder_detection_pipeline",
    "runtime": "python",
    "graph": ["(ReminderSubsystemElement)", "(SpeechToTextElement)", "(TextToSpeechElement)"],
    "parameters": {},
    "elements": [
        {
            "name": "ReminderSubsystemElement",
            "input": [{"name": "reminder_text", "type": "str"}],
            "output": [{"name": "processed_reminder", "type": "str"}],
            "deploy": {"local": {"module": "cos40006.elements.reminder_subsystem_element"}}
        },
        {
            "name": "SpeechToTextElement",
            "input": [{"name": "audio_input", "type": "str"}],
            "output": [{"name": "text_output", "type": "str"}],
            "deploy": {"local": {"module": "cos40006.elements.speech_to_text_element"}}
        },
        {
            "name": "TextToSpeechElement",
            "input": [{"name": "text_input", "type": "str"}],
            "output": [{"name": "audio_output", "type": "str"}],
            "deploy": {"local": {"module": "cos40006.elements.text_to_speech_element"}}
        }
    ]
}